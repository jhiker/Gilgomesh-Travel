    restrict: "A"
    require: "ngModel"
    link: (scope, element, attrs, ngModelCtrl) ->
      startdate=new Date()
      enddate = new Date(new Date().setMonth(new Date().getMonth()+3))
      if attrs.ngModel is "return"
        elements=["#depart", "maxDate"]
        if "depart" of scope then startdate=new Date(scope.depart)
      else
        elements=["#return", "minDate"]
      $ ->
        element.datepicker
          dateFormat: "mm/dd/yy"
          minDate: startdate
          maxDate: enddate
          onSelect: (date) ->
            scope.$apply ->
              ngModelCtrl.$setViewValue date
              $(elements[0]).datepicker "option", elements[1], new Date(date)
