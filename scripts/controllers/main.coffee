    $scope.$watch "destination", ->
      if $scope.destination in $scope.city_names
      	$scope.cityvalid+=1 
    $scope.$watch "origin", ->
      if $scope.origin in $scope.city_names
      	$scope.cityvalid+=1
    if $scope.cityvalid ==2
    	$scope.buildmap()
    $scope.getWidth = ->
      $(window).width()
    $scope.getHeight = ->
      $(window).height()
    $scope.$watch $scope.getWidth, (newValue, oldValue) ->
      $scope.window_width = newValue
    $scope.$watch $scope.getHeight, (newValue, oldValue) ->
      $scope.window_height = newValue
    window.onresize = ->
      $scope.$apply()
    $scope.clearmap = () ->
      m= $scope.map
      for i of m._layers
        unless m._layers[i]._path is `undefined`
          try
            m.removeLayer m._layers[i]
          catch e
            console.log "problem with " + e + m._layers[i]
      return 
    $scope.search = ->
      getSpace=(point) ->
        key = "("+point[0]+", "+point[1]+")"#lat, long
        $scope.grid[key]
      getSlope= (point1, point2)-> 
        parseFloat((point2[1]-point1[1])/(point2[0]-point1[0]))
      getTravelElipse= (cities)-> 
        if Math.abs(cities[1][1]-cities[0][1])>Math.abs(cities[1][0]-cities[0][0]) then flip = true else flip = false 
        console.log(flip)
        if flip
          cities[i].reverse() for i in [0,1]
        [[x1, y1], [x2, y2]] = cities
        y_float=y1
        elipse= {}
        path_cities={}
        slope = getSlope([x1, y1], [x2, y2])
        for x_count in [x1...x2]
          depth= Math.min(x_count+1-x1, x2-x_count+1)
          for lower in [0...depth]
            for sign in [-1, 1]
              newitem = [x_count, Math.round(y_float+sign* lower)]
              newitem.reverse() if flip 
              if newitem not in elipse
                elipse[newitem]= null 
                newcities = getSpace(newitem)
                if newcities
                  for newcity in newcities
                    if newcity not in path_cities+[$scope.origin + $scope.destination]
                      path_cities[newcity] = null
          y_float+=slope
        path_cities
      getPoints= (origin, destination)->
        @cities = []
        for name in [origin, destination]
          @cities.push([parseFloat($scope.cities[name][0]), parseFloat($scope.cities[name][1])])
        @flip=false
        if @cities[0][0]>@cities[1][0]
          @flip= true 
          @cities.reverse()
        @cities= (Math.round(lat_lng) for lat_lng in city for city in @cities) 
        if @cities[0][0]==@cities[1][0] then @cities[1][0]=@cities[1][0]+1
      $scope.buildmap()
      points= new getPoints($scope.origin, $scope.destination)
      $scope.travelpath =getTravelElipse(points.cities)
      #could be refactored into OOP

      console.log($scope.travelpath)
      $scope.searched=1
      
      map = $scope.map
      connection_line  = {color: 'blue', weight: 1, opacity: .5}
      $scope.midpoints =[]
      for midpoint of $scope.travelpath
        midlatlng = $scope.cities[midpoint].map(parseFloat)
        forward= L.polyline([$scope.ltlngs[0],midlatlng ], connection_line).addTo(map)
        #How to remove?
        backward =L.polyline([midlatlng,$scope.ltlngs[1] ], connection_line).addTo(map)
        for item in [forward, backward]
        	item.addEventListener "click", ->
	        	$scope.map.removeLayer(forward)
	        	$scope.map.removeLayer(backward)
        $scope.midpoints.concat([forward, backward])
    $scope.buildmap = ->
      map= $scope.map
      $scope.ltlngs=[]
      for name in [$scope.origin, $scope.destination]
        $scope.ltlngs.push(parseFloat(num) for num in $scope.cities[name])
      console.log($scope.ltlngs)
      polyline = L.polyline($scope.ltlngs, {color: 'red', weight: 10, opacity: .95}).addTo(map)
      map.fitBounds(polyline.getBounds())
    $scope.goback = ->
      $scope.travelpath={}
      $scope.searched=0
      $scope.cityvalid = 0
      $scope.clearmap()
