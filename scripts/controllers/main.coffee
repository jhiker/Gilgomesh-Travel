	   restrict: "A"
	   link: (scope, element, attr) ->
	     $(element).backstretch attr.backgroundUrl)
   .directive("map", ->
      restrict: "E"
      link: (scope) ->
        map = L.map("map").setView([41.505, -0.81], 4)
        L.tileLayer("http://{s}.tile.cloudmade.com/BC9A493B41014CAABB98F0471D759707/997/256/{z}/{x}/{y}.png",
          maxZoom: 5
          minZoom: 2
        ).addTo map
        scope['map']=map)
   .directive("slider", ->
      restrict: "A"
      require: "ngModel" 
      link: (scope) ->
        $(document).ready ->
          $(".slider").slider
            min: 0
            max: 5
            step: 1
            handle: 'round'
            orientation: "horizontal"
            selection: "after"
          return)
        
